{
  "plugins": [ "theme-api", "-sharing", "toolbar" ],
  "pluginsConfig": {
    "theme-api": {
      "languages": [
        {
          "lang": "js",
          "name": "JavaScript",
          "default": true
        }
      ]
    },
    "toolbar": {
      "buttons": [
        {
          "label": "GitHub",
          "icon": "fa fa-github",
          "url": "https://github.com/MeadCo/ScriptX.Print.Client"
        },
        {
          "label": "NPM",
          "icon": "fa fa-bullseye",
          "url": "https://www.npmjs.com/package/scriptxprint-html"
        }
      ]
    }
   },
  "root": "./docs",
  "gitbook": "3.2.2",
  "structure": {
    "readme": "readme.md",
    "summary": "summary.md"
  }
}
