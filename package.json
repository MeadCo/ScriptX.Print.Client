{
  "name": "scriptxprint-html",
  "version": "1.15.2",
  "description": "General library and emulation of ScriptX.Add-on for use with ScriptX.Services",
  "main": "meadco-scriptxprinthtml.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/MeadCo/ScriptX.Print.Client"
  },
  "author": "Pete Cole <pcole@meadroid.com>",
  "license": "MIT",
  "keywords": [
    "meadco",
    "scriptx",
    "meadco-scriptx",
    "printing",
    "printing-html",
    "printing-pdf",
    "printing-raw",
    "printing-direct",
    "promptless"
  ],
  "homepage": "https://scriptxservices.meadroid.com/",
  "files": [
    "src/*.js",
    "dist/*.js"
  ],
  "scripts": {
    "dist": "gulp BuildDist",
    "docs:build": "gulp BuildDocs",
    "docs:dev": "npm-run-all --parallel watch:docs view:docs",
    "watch:docs": "onchange \"docs-src/**/*.*\" \"src/**/*.js\" -- npm run docs:build",
    "view:docs": "http-server docs -o index.html"
  },
  "devDependencies": {
    "@jsdoc/salty": "^0.2.8",
    "better-docs": "^2.3.2",
    "clean-css": "4.2.3",
    "del": "5.1.0",
    "eslint": "^8.50.0",
    "gulp": "~4.0.2",
    "gulp-concat": "^2.6.1",
    "gulp-cssmin": "^0.2.0",
    "gulp-htmlmin": "^5.0.1",
    "gulp-jsdoc3": "^3.0.0",
    "gulp-replace": "^1.0.0",
    "gulp-terser": "^2.1.0",
    "ini": ">=1.3.6",
    "jsdoc": "^3.6.5",
    "lodash.template": ">=4.5.0",
    "merge-stream": "^2.0.0",
    "minimist": "^1.2.5",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.0.2",
    "readable-stream": "^3.6.0",
    "yargs-parser": "18.1.3"
  },
  "dependencies": {
    "jquery": ">=3.4.0",
    "pug-code-gen": ">=2.0.3",
    "y18n": ">=3.2.2"
  }
