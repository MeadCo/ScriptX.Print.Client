<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>netcoreapp2.2</TargetFramework>
    <AspNetCoreHostingModel>InProcess</AspNetCoreHostingModel>
    <RootNamespace>ScriptX.Services_Client</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Pages\Tests\printpdf.js" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="Pages\Tests\printpdf.js">
      <CopyToPublishDirectory>PreserveNewest</CopyToPublishDirectory>
    </Content>
  </ItemGroup>


  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.App" />
    <PackageReference Include="Microsoft.AspNetCore.Razor.Design" Version="2.2.0" PrivateAssets="All" />
    <PackageReference Include="Microsoft.VisualStudio.Web.CodeGeneration.Design" Version="2.2.0" />
  </ItemGroup>


  <ItemGroup>
    <Reference Include="MeadCo.ScriptX.Print.Messaging">
      <HintPath>..\..\Libraries\MeadCo.ScriptX.Print.Messaging.dll</HintPath>
    </Reference>
  </ItemGroup>

</Project>
