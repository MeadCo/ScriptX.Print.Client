@page
@{
    ViewData["Title"] = "-Core.js";
}

<h1>@ViewData["Title"]</h1>

@section Scripts {
    <environment include="Development">
        <script asp-append-version="true" src="/scripts?name=src/meadco-core.js"></script>
    </environment>
    <environment exclude="Development">
        <script asp-append-version="true" src="/scripts?name=dist/meadco-scriptxservicesprint.min.js"></script>
    </environment>
}

@section TestScripts {
    <script asp-append-version="true" src="~/js/UnitTests/Core.js"></script>
}
