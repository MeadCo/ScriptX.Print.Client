@page
@{
    ViewData["Title"] = "-secmgr.js";
}

<h1>@ViewData["Title"]</h1>

@section Scripts {

    <environment include="Development">
        <script src="/scripts/src/meadco-core.js"></script>
        <script src="/scripts/src/meadco-scriptxprint.js"></script>
        <script src="/scripts/src/meadco-scriptxprintlicensing.js"></script>
        <script src="/scripts/src/meadco-secmgr.js"></script>
    </environment>
    <environment exclude="Development">
        <script src="/scripts/dist/meadco-scriptxservices.min.js"></script>
    </environment>
}

@section TestScripts {
    <script src="~/js/UnitTests/serverinfo.js" asp-append-version="true"></script>
    <script src="~/js/UnitTests/secmgr.js"></script>
}
