@page
@{
    ViewData["Title"] = "-PrintPDFjs";
}

<h1>@ViewData["Title"]</h1>


@section Scripts {
    <environment include="Development">
        <script src="/scripts/src/meadco-core.js"></script>
        <script src="/scripts/src/meadco-scriptxprint.js"></script>
        <script src="/scripts/src/meadco-scriptxprintpdf.js"></script>
    </environment>
    <environment exclude="Development">
        <script src="/scripts/dist/meadco-scriptxservicesprint.min.js"></script>
    </environment>
}
@section TestScripts {
    <script src="~/js/UnitTests/serverinfo.js" asp-append-version="true"></script>
    <script src="~/js/UnitTests/PrintPdf.js"></script>
}

<iframe style="visibility: hidden; display: none" id="testFrame" src="@Url.Page("/Tests/Frame")"></iframe>
<iframe style="visibility: hidden; display: none" name="testFrame-2" src="@Url.Page("/Tests/Frame2")"></iframe>
